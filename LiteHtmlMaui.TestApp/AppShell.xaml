<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="LiteHtmlMaui.TestApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:LiteHtmlMaui.TestApp"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Text Comparison"
        Route="TextComparison" 
        ContentTemplate="{DataTemplate local:TextComparisonPage}"
        />

    <ShellContent
        Title="Text"
        Route="Text" 
        ContentTemplate="{DataTemplate local:TextPage}"
        />

    <ShellContent
        Title="Borders"
        Route="Borders" 
        ContentTemplate="{DataTemplate local:BordersPage}"
        />


    <ShellContent
        Title="Images"
        ContentTemplate="{DataTemplate local:ImagesPage}"
        Route="Images" />

    

</Shell>
